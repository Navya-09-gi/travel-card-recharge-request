        <messages/>
        <name>validation for visa copy</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
   if (isLoading || newValue === '') {
      return;
   }

   // Check if something was uploaded
   if (!newValue) {
      g_form.showFieldMsg('visa_copy', 'Visa copy is mandatory.', 'error');
      return;
   }

   // Try checking extension using field value (this is limited)
   var fileName = newValue.toLowerCase();
   if (!fileName.endsWith(".pdf")) {
      g_form.showFieldMsg('visa_copy', 'Only PDF documents are allowed.', 'error');
      g_form.setValue('visa_copy', ''); // Optional: Clear the invalid file reference
      return;
   }

   // All good
   g_form.hideFieldMsg('visa_copy', true);
}
]]></script>
