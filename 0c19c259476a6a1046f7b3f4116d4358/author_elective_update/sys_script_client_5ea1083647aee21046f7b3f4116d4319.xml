        <messages/>
        <name>validation of amount to be loaded(no)</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading || newValue == '') {
        return;
    }

    // Clear previous messages
    g_form.hideFieldMsg('amount_to_be_loaded', true);

    // Check if input is not a number
    if (!/^\d+(\.\d{1,2})?$/.test(newValue)) {
        g_form.showFieldMsg(
            'amount_to_be_loaded', // Field name
            '❌ Only numeric values are allowed. Please enter a valid amount.', // Message
            'error' // Message type (error, info, warning)
        );
        g_form.setValue('amount_to_be_loaded', ''); // Clear the field
    }
}]]></script>
